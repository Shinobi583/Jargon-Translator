{
  "name": "Jargon-Translator",
  "description": "Translate all that confusing Jargon!",
  "version": "1.0.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "img/face.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "author": "Chace DeLoach"
}